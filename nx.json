    },
    "blockchain-bsc-dapp-example": {
      "tags": ["binance smartchain dapp"]
    },
    "blockchain-ethereum-dapp-example": {
      "tags": ["ethereum dapp"]
    },
    "code-generator": {
      "tags": []
    },
    "codegenerator": {
      "tags": []
    },
    "counter": {
      "tags": []
    },
    "general-playground": {
      "tags": []
    }
  }
